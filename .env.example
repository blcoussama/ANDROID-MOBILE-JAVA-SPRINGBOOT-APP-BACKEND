# Database PostgreSQL
# Pour d√©veloppement local: jdbc:postgresql://localhost:5432/cabinet_medical
# Pour production Railway: Fourni automatiquement par Railway
DATABASE_URL=jdbc:postgresql://localhost:5432/your_database_name
PGUSER=your_database_user
PGPASSWORD=your_database_password

# Server Port
# Railway fournit automatiquement le PORT en production
PORT=8080

# JWT Secret
# ‚ö†Ô∏è S√âCURIT√â: Utiliser un secret FORT en production!
# G√©n√©rer avec: openssl rand -base64 32
JWT_SECRET=your_strong_random_secret_key_here_minimum_32_characters

# JWT Expiration (en millisecondes)
# 3600000 ms = 1 heure
JWT_EXPIRATION=3600000
